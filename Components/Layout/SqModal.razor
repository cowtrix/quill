<div class="sq-modal-wrapper">
    <div class="sq-modal-dismiss" @onclick="OnDismiss" />
    <div class="sq-card sq-modal">
        @ChildContent
        <SqIconButton Icon="check-lg" Label="Submit" OnClick="OnSubmit" />
    </div>
</div>

@code {
    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter]
    public EventCallback OnDismiss { get; set; }

    [Parameter]
    public EventCallback OnSubmit { get; set; }
}
